@{
    ViewBag.Title = "Academia Transforma";
}
<div class="container principal">
    <div class="row  justify-content-around align-items-center">
        <div class="espa col-md-5 col-12">
            <div id="carouselExampleCaptions" class="carousel slide" data-bs-ride="carousel">
                <div class="carousel-indicators">
                    <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="0" class="active" aria-current="true" aria-label="Slide 1"></button>
                    <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="1" aria-label="Slide 2"></button>
                    <button type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide-to="2" aria-label="Slide 3"></button>
                </div>
                <div class="carousel-inner" data-bs-interval="3000">
                    <div class="carousel-item active">
                        <img src="midia/800x800x4.png" class="arredondamento-top d-block w-100" alt="...">
                        <div class="carousel-caption d-none d-md-block">
                        </div>
                    </div>
                    <div class="carousel-item" data-bs-interval="3000">
                        <img src="midia/800x800x2.png" class="arredondamento-top d-block w-100" alt="...">
                        <div class="carousel-caption d-none d-md-block">
                        </div>
                    </div>
                    <div class="carousel-item" data-bs-interval="3000">
                        <img src="midia/800x800x3.png" class="arredondamento-top d-block w-100" alt="...">
                        <div class="carousel-caption d-none d-md-block">
                        </div>
                    </div>
                </div>
                <button class="carousel-control-prev" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="prev">
                    <span class="carousel-control-prev-icon" aria-hidden="true"></span>
                    <span class="visually-hidden">Previous</span>
                </button>
                <button class="carousel-control-next" type="button" data-bs-target="#carouselExampleCaptions" data-bs-slide="next">
                    <span class="carousel-control-next-icon" aria-hidden="true"></span>
                    <span class="visually-hidden">Next</span>
                </button>
            </div>
        </div>
        <div class="escrito-principal prof-escrito col-md-5 col-12 espa ">
            <h1>AQUI VOCÊ ENCONTRA A MELHOR E MAIS VARIADAS FORMAS DE MELHORAR SUA SAÚDE!</h1>
        </div>
    </div>

    <div class="row quadro justify-content-around">
        <div class="variedades col-12">
            <h1 class="fs-2 font-weight-bolder">ALGUMAS VARIEDADES DE SERVIÇOS</h1>
        </div>
        <div class="tam cartao col-12 col-md-6 col-lg-3 col-sm-12">
            <img class="tamanho-foto arredondamento" src="midia/pilates-certo.png" alt="">
            <h1 class="fs-4 nome-cartao font-weight-bold">PILATES</h1>
        </div>
        <div class="tam cartao col-12 col-md-6 col-lg-3 col-sm-12">
            <img class="tamanho-foto arredondamento" src="midia/natação-certo.png" alt="">
            <h1 class="fs-4 nome-cartao font-weight-bold">NATAÇÃO</h1>
        </div>
        <div class="tam cartao col-12 col-md-6 col-lg-3 col-sm-12">
            <img class="tamanho-foto arredondamento" src="midia/miscula-certo.png" alt="">
            <h1 class="fs-4 nome-cartao font-weight-bold">MUSCULAÇÃO</h1>
        </div>
        <div class="tam cartao col-12 col-md-6 col-lg-3 col-sm-12">
            <img class="tamanho-foto arredondamento" src="midia/crossfit.-certojpg.png" alt="">
            <h1 class="fs-4 nome-cartao font-weight-bold">CROSSFIT</h1>
        </div>
    </div>
    <div class="row prof-quadro align-items-center justify-content-center">

        <div class="foto-prof-div col-lg-6 col-sm-12">
            <img class="tamanho-prof" src="midia/PROF.jpg" alt="">
        </div>
        <div class="col-lg-6 col-sm-12">
            <h1 class="prof-escrito">
                <p class="fs-1">Academia Transforma</p>
                <p class="fs-2">
                    Pilates com Studio Completo
                    Musculação com Atendendimento Personalizado,
                    Hidroginástica com Água Aquecida e
                    profissionais qualificados para melhor atendelos!
                </p>
            </h1>
        </div>
    </div>
    <div id="cadastro" class="container contatos">
        <h1 class="contato-titulo">MEIOS DE CONTATOS</h1>
        <a target="_blank" class="row justify-content-center align-items-center botao-zap-feito" href="https://contate.me/17-99787-5150"></a>
        <div class="row justify-content-center align-items-center botao-zap">
            <div class="col-12 zap-logo">
                <img class="foto-zap-img" src="midia/zapzap-250.png" alt="">
            </div>
            <div class="escrito-zap col-12">
                <h3>DÚVIDAS</h3>
            </div>
        </div>
    </div>

    <div class="row linha-face justify-content-around">

        <div class="foto-ins col-6">
            <a target="_blank" href="https://www.facebook.com/AcademiaTransforma"><img class="face-iinst" src="midia/face250.png" alt=""></a>
        </div>

        <div class="foto-ins col-6">
            <a target="_blank" href="https://www.instagram.com/transforma.academia/"><img class="face-iinst" src="midia/insta250.png" alt=""></a>
        </div>

    </div>

    <h2 class="fonte centros loca">Localização</h2>
    <div class="mapa-config">
        <iframe class="borda-mapa" src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d14885.426063345629!2d-48.98530967039028!3d-21.13820572502541!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x94bc1e5e68b6092b%3A0x9b161c8bfb05edb1!2sAcademia%20Transforma!5e0!3m2!1spt-BR!2sbr!4v1663885539684!5m2!1spt-BR!2sbr" width="80%" height="250" style="border:0;" allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
    </div>
    <div class="contatos">
        <h3 class="fonte centros font-weight-bolder">Rua Aracajú - 1329, Centro</h3>
        <h3 class="fonte centros">Catanduva-sp</h3>
    </div>

</div>
@if (TempData["MsgRecusado"] != null)
{
    <script type="text/javascript">
        alert("Login invalido, você foi direcionado para tela Principal");
    </script>

}
